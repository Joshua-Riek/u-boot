#include "rk3588-u-boot.dtsi"

/ {
	aliases {
		mmc0 = &sdhci;
	};

	chosen {
		u-boot,spl-boot-order = &sdhci;
	};
};

&sdhci {
	cap-mmc-highspeed;
	mmc-ddr-1_8v;
	mmc-hs200-1_8v;
};

&sdmmc {
	status = "disabled";
};

&uart9 {
	clock-frequency = <24000000>;
	bootph-pre-ram;
	status = "okay";
};

